<?xml version="1.0"?>
<prefab>
  <eElement alignx="MinMax" aligny="MinMax">
    <cReceiver mute="true" />
    <cScript>
      if not scene then
        scene = entity
        entity.find_component("cReceiver").set_mute(false)
      end
    </cScript>
    <eNode name="node_root" >
      <cScript src="shoot_ball.lua plane_tree.lua" >
        node_root = entity
      </cScript>
      <entity src="D:\sky\sky" name="sky" />
      <eCube pos="200,60,200" dynamic="false" />
      <eCube pos="210,60,200" dynamic="false" />
      <eCube pos="200,60,205" dynamic="false" />
      <entity src="D:\character\character" name="character" pos="200,65,200" >
        <cController radius="0.5" height="1.8" />
        <cScript src="character_controller.lua" />
        <eNode name="camera" pos="0,5,0" euler="0,0,0" >
          <cCamera current="true" />
          <cScript src="camera_third.lua" />
        </eNode>
      </entity>
      <entity src="D:\terrain\terrain" name="terrain" />
      <eLight name="sun" euler="0,90,0" type="Directional" color="1.0,1.0,1.0" cast_shadow="true" />
    </eNode>
    <eElement name="post_effect" >
      <cPostEffect enable_bloom="true" />
    </eElement>
  </eElement>
</prefab>
