<?xml version="1.0"?>
<prefab>
  <entity>
    <cElement alignx="MinMax" aligny="MinMax" />
    <cNode />
    <entity name="scene">
      <cNode />
      <cScript src="tests/shoot_ball.lua" />
      <entity src="D:\sky\sky" />
      <entity src="prefabs/cube">
        <cNode pos="0,200,0" />
        <cRigid dynamic="false" />
      </entity>
      <entity src="prefabs/cube">
        <cNode pos="10,200,0" />
        <cRigid dynamic="false" />
      </entity>
      <entity src="prefabs/cube">
        <cNode pos="0,200,5" />
        <cRigid dynamic="false" />
      </entity>
      <entity src="D:\character\character" >
        <cNode pos="0,205,0" />
        <cController radius="0.5" height="1.8" />
        <cScript src="character_controller.lua" />
        <entity name="camera">
          <cNode pos="0,5,0" euler="0,0,0" />
          <cCamera current="true" />
          <cScript src="camera_third.lua" />
        </entity>
      </entity>
      <entity src="D:\terrain\terrain">
        <cNode pos="-100,100,-100" />
      </entity>
      <entity>
        <cNode euler="0,60,0" />
        <cLight type="Directional" color="1.0,1.0,1.0" cast_shadow="true" />
      </entity>
    </entity>
    <entity>
      <cElement />
      <cPostEffect enable_bloom="true" />
    </entity>
    <entity name="debug">
      <cElement />
    </entity>
    <eElement name="ui" alignx="MinMax" aligny="MinMax" >
      <eMenuBar>
        <eMenu text="shading" >
		      <eMenuItem text="solid" >
            <cScript>
              entity:find_component("cReceiver"):add_mouse_left_down_listener(function()
		            cmd:excute("view_shading_solid")
	            end)
            </cScript>
          </eMenuItem>
		      <eMenuItem text="wireframe" >
            <cScript>
              entity:find_component("cReceiver"):add_mouse_left_down_listener(function()
                cmd:excute("view_shading_wireframe")
              end)
            </cScript>
          </eMenuItem>
        </eMenu>
        <eMenu text="physics" >
		      <eMenuItem text="visualization_on" >
            <cScript>
              entity:find_component("cReceiver"):add_mouse_left_down_listener(function()
                cmd:excute("view_physics_on")
              end)
            </cScript>
          </eMenuItem>
		      <eMenuItem text="visualization_off" >
            <cScript>
              entity:find_component("cReceiver"):add_mouse_left_down_listener(function()
                cmd:excute("view_physics_off")
              end)
            </cScript>
          </eMenuItem>
        </eMenu>
      </eMenuBar>
    </eElement>
  </entity>
</prefab>
